{
    "openapi": "3.0.0",
    "info": {
        "title": "Asparecargas Endpoints",
        "description": "Here are the endpoints of Asparecargas",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://127.0.0.1:8000"
        }
    ],
    "paths": {
        "/api/v1/products": {
            "get": {
                "tags": [
                    "Products"
                ],
                "summary": "Get enabled products",
                "operationId": "52f07b5c504cf5dead737602311e23a8",
                "responses": {
                    "200": {
                        "description": "Get enabled products",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "properties": {
                                                    "type": {
                                                        "type": "string",
                                                        "example": "products"
                                                    },
                                                    "attributes": {
                                                        "properties": {
                                                            "id": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "product_name": {
                                                                "type": "string",
                                                                "example": "BANCOLOMBIA"
                                                            },
                                                            "product_description": {
                                                                "type": "string",
                                                                "example": "<p>DEPOSITO BANCOLOMBIA</p>"
                                                            },
                                                            "product_logo": {
                                                                "type": "string",
                                                                "example": "/storage/products/1.png"
                                                            },
                                                            "product_commission": {
                                                                "type": "integer",
                                                                "example": 800
                                                            },
                                                            "client_name": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "client_document": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "phone_number": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "email": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "account_type": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "account_number": {
                                                                "type": "integer",
                                                                "example": 1
                                                            },
                                                            "code": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "extra": {
                                                                "type": "integer",
                                                                "example": 0
                                                            },
                                                            "min_amount": {
                                                                "type": "integer",
                                                                "example": 5000
                                                            },
                                                            "max_amount": {
                                                                "type": "integer",
                                                                "example": 300000
                                                            },
                                                            "category": {
                                                                "type": "string",
                                                                "example": "bancos"
                                                            }
                                                        },
                                                        "type": "object"
                                                    }
                                                },
                                                "type": "object"
                                            }
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                },
                "security": [
                    {
                        "passport": []
                    }
                ]
            }
        },
